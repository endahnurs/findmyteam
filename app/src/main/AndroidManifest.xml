    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".SignInActivity" android:theme="@style/Main">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" android:theme="@style/Main"></activity>
        <activity android:name=".MainActivity" android:theme="@style/Main"></activity>
        <activity android:name=".NewsCompActivity"></activity>
        <activity android:name=".TeamViewActivity"></activity>
    </application>

</manifest>